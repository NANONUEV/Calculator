          />
          <div class="calculator-container2">
            <button onclick="insertValue('Math.sin(')" class="calculator-button button">SIN</button>
            <button onclick="insertValue('Math.cos(')" class="calculator-button01 button">COS</button>
            <button onclick="insertValue('Math.tan(')" class="calculator-button02 button">TAN</button>
            <button onclick="insertValue('Math.sqrt(')" class="calculator-button03 button">SQRT</button>
          </div>
          <div class="calculator-container3">
            <button onclick="clearResult()" class="calculator-button04 button">C</button>
            <button onclick="backspace()" class="calculator-button05 button">CE</button>
