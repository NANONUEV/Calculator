          </div>
          <div class="calculator-container5">
            <button onclick="insertValue('4')" class="calculator-button12 button">4</button>
            <button onclick="insertValue('5')" class="calculator-button13 button">5</button>
            <button onclick="insertValue('6')" class="calculator-button14 button">6</button>
            <button onclick="insertValue('*')" class="calculator-button15 button">*</button>
          </div>
          <div class="calculator-container6">
            <button onclick="insertValue('1')" class="calculator-button16 button">1</button>
            <button onclick="insertValue('2')" class="calculator-button17 button">2</button>
            <button onclick="insertValue('3')" class="calculator-button18 button">3</button>
            <button onclick="insertValue('-')" class="calculator-button19 button">-</button>
          </div>
          <div class="calculator-container7">
            <button onclick="insertValue('.')" class="calculator-button20 button">.</button>
            <button onclick="insertValue('0')" class="calculator-button21 button">0</button>
            <button onclick="calculate()" class="calculator-button22 button">=</button>
            <button onclick="insertValue('+')" class="calculator-button23 button">+</button>
          </div>
        </div>
      </div>
    </div>
    <script>
    let result = document.getElementById("result");

function insertValue(value) {
    result.value += value;
}

function clearResult() {
    result.value = "";
}

function backspace() {
    result.value = result.value.slice(0, -1);
}

function calculate() {
    let expression = result.value;
    let answer = eval(expression);
    result.value = answer;
}
    </script>
  </body>
</html>
